{
  "name": "riitag",
  "version": "2.0.0",
  "repository": {
    "url": "https://github.com/WiiDatabase/riitagNext"
  },
  "license": "AGPL-3.0-only",
  "scripts": {
    "lint": "next lint",
    "prettier": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. .",
    "prettier:format": "prettier --ignore-path .prettierignore --plugin-search-dir=. --write .",
    "dev:start": "next dev",
    "dev:db-create-migration": "dotenv -e .env.development.local prisma migrate dev",
    "dev:db-reset": "dotenv -e .env.development.local prisma db push --force-reset --accept-data-loss",
    "dev:db-pull": "dotenv -e .env.development.local prisma db pull",
    "dev:db-generate-client": "dotenv -e .env.development.local prisma generate",
    "dev:db-validate": "dotenv -e .env.development.local prisma validate",
    "prod:migrate": "dotenv -e .env.production.local prisma migrate deploy",
    "prod:build": "next build",
    "prod:start": "next start",
    "global:check-dependencies": "npm-check --skip-unused"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "2.8.1",
    "prisma": "4.8.0",
    "sass": "1.57.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@next/bundle-analyzer": "^14.1.0",
    "@prisma/client": "4.8.0",
    "@tiptap/extension-link": "2.0.0-beta.209",
    "@tiptap/react": "2.0.0-beta.209",
    "@tiptap/starter-kit": "2.0.0-beta.209",
    "bootstrap": "5.2.3",
    "canvas": "^2.11.2",
    "dayjs": "1.11.7",
    "dotenv-cli": "6.0.0",
    "form-data": "4.0.0",
    "formidable": "2.1.1",
    "formik": "2.2.9",
    "html-react-parser": "^4.2.2",
    "iron-session": "6.3.1",
    "lru-cache": "7.14.1",
    "moment": "^2.29.4",
    "next": "^14.1.0",
    "next-connect": "0.13.0",
    "next-seo": "5.15.0",
    "nextjs-progressbar": "0.0.16",
    "pino": "8.8.0",
    "pino-pretty": "^10.3.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-bootstrap": "2.7.0",
    "react-dom": "18.2.0",
    "react-paginate": "8.1.4",
    "react-toastify": "9.1.1",
    "safe-json-stringify": "1.2.0",
    "swr": "2.0.0",
    "twitter-lite": "1.1.0",
    "xml2js": "^0.6.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
