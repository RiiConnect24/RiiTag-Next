{
  "name": "riitag",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/WiiDatabase/RiiTag-Next"
  },
  "license": "AGPL-3.0-only",
  "scripts": {
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && next lint",
    "dev:start": "next dev",
    "dev:db-create-migration": "dotenv -e .env.development.local prisma migrate dev",
    "dev:db-reset": "dotenv -e .env.development.local prisma db push --force-reset --accept-data-loss",
    "dev:db-pull": "dotenv -e .env.development.local prisma db pull",
    "dev:db-generate-client": "dotenv -e .env.development.local prisma generate",
    "prod:migrate": "dotenv -e .env.production.local prisma migrate deploy",
    "prod:build": "next build",
    "prod:start": "next start",
    "global:check-dependencies": "npm-check --skip-unused"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-unicorn": "39.0.0",
    "prettier": "2.5.1",
    "prisma": "3.6.0",
    "sass": "1.45.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@next/bundle-analyzer": "12.0.7",
    "@prisma/client": "3.6.0",
    "@tiptap/extension-link": "2.0.0-beta.28",
    "@tiptap/react": "2.0.0-beta.95",
    "@tiptap/starter-kit": "2.0.0-beta.144",
    "bootstrap": "5.1.3",
    "canvas": "2.8.0",
    "dayjs": "1.10.7",
    "dotenv-cli": "4.1.1",
    "form-data": "4.0.0",
    "formik": "2.2.9",
    "iron-session": "6.0.4",
    "lru-cache": "6.0.0",
    "next": "12.0.7",
    "next-connect": "0.11.0",
    "nextjs-progressbar": "0.0.13",
    "next-seo": "4.28.1",
    "pino": "7.5.1",
    "pino-pretty": "7.2.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-bootstrap": "2.0.3",
    "react-dom": "17.0.2",
    "react-paginate": "8.1.0",
    "safe-json-stringify": "1.2.0",
    "react-toastify": "8.1.0",
    "swr": "1.1.1",
    "twitter-lite": "1.1.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
